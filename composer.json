{
  "name": "codeinwp/tweet-old-post",
  "description": "Tweet Old Post for WordPress.",
  "keywords": [
    "wordpress",
    "share"
  ],
  "homepage": "https://themeisle.com/plugins/tweet-old-post/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "ThemeIsle Team",
      "email": "friends@themeisle.com.com",
      "homepage": "https://themeisle.com"
    }
  ],
  "type": "wordpress-plugin",
  "support": {
    "issues": "https://github.com/Codeinwp/tweet-old-post/issues",
    "source": "https://github.com/Codeinwp/tweet-old-post"
  },
  "autoload": {
    "files": [
      "vendor/codeinwp/themeisle-sdk/load.php"
    ],
    "classmap": [
      "dependencies/classes"
    ]
  },
  "require": {
    "facebook/graph-sdk": "5.7",
    "abraham/twitteroauth": "1.1.0",
    "codeinwp/themeisle-sdk": "3.2.15",
    "google/apiclient": "2.7.1"
  },
  "scripts": {
    "post-install-cmd": [
      "Google_Task_Composer::cleanup",
      "\"vendor/bin/mozart\" compose",
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "Google_Task_Composer::cleanup",
      "\"vendor/bin/mozart\" compose",
      "composer dump-autoload"
    ]
  },
  "extra": {
  "installer-disable": true,
  "google/apiclient-services": [
    "AnalyticsReporting"
  ],
  "mozart": {
    "dep_namespace": "ROP\\Dependencies\\",
    "dep_directory": "/dependencies/packages/",
    "classmap_directory": "/dependencies/classes/",
    "classmap_prefix": "ROP_Dependency",
    "packages": [
        "facebook/graph-sdk",
        "abraham/twitteroauth",
        "codeinwp/themeisle-sdk",
        "google/apiclient"

    ],
    "override_autoload": {
      "google/apiclient": {
          "classmap": [
              "src/"
          ]
      }
  },
    "delete_vendor_directories": true
}
  },
  "require-dev": {
    "coenjacobs/mozart": "^0.5.1"
  }
}
